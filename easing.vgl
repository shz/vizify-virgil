struct PolyEngine {
  f : lambda(x:float) : float = null
  df : lambda(x:float) : float = null
}

function poly(n1:float, n2:float) :  {
  let c = 3 * n1
  let b = 3 * (n2 - n1) - c
  let a = 1 - c - b

  return new PolyEngine({
    f = lambda(x:float) { return x * (c + x * (b + x * a)) }
    df = lambda(x:float) { return c + x * (2 * b + 3 * a * x) }
  })
}

